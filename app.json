{
  "expo": {
    "name": "RealEstate",
    "slug": "RealEstate",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/LoginImages/splash.png"
    },
    "extra": {
      "eas": {
        "projectId": "545913ea-09d1-4128-bafb-42c81dc4295d"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": false
        }
      ]
    ]
  }
}
